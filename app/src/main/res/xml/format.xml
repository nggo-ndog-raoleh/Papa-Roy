    >
    <Row>
        <Key android:codes="1" android:keyLabel="Format pemesanan" android:keyWidth="94%p" android:horizontalGap="3%p" android:keyEdgeFlags="left"/>
    </Row>
    <Row>
        <Key android:codes="2" android:keyLabel="Format Pembayaran" android:keyWidth="94%p" android:horizontalGap="3%p" android:keyEdgeFlags="left" />
    </Row>
    <Row>
        <Key android:codes="3" android:keyLabel="Ganti Keyboard" android:keyWidth="94%p" android:horizontalGap="3%p" android:keyEdgeFlags="left" />
    </Row>
    <Row>
        <Key android:codes="4" android:keyLabel="Previous"/>
        <Key android:codes="5" android:keyLabel="Next"/>
    </Row>
